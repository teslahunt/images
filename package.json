{
  "name": "tesla-images",
  "description": "Get Tesla vehicles images from Tesla inventory or 3D visualization",
  "homepage": "https://nicedoc.io/teslahunt/tesla-images",
  "version": "1.1.1",
  "main": "index.js",
  "author": {
    "email": "hello@teslahunt.io",
    "name": "Tesla Hunt",
    "url": "https://teslahunt.io"
  },
  "contributors": [
    {
      "name": "Kiko Beats",
      "email": "josefrancisco.verdu@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teslahunt/tesla-images.git"
  },
  "bugs": {
    "url": "https://github.com/teslahunt/tesla-images/issues"
  },
  "keywords": [
    "images",
    "tesla",
    "tesla-api",
    "teslaapi",
    "teslamotors"
  ],
  "dependencies": {
    "cheerio": "~1.0.0-rc.10",
    "got": "~11.8.2",
    "p-reflect": "~2.1.0",
    "reachable-url": "~1.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "lint-staged": "latest",
    "npm-check-updates": "latest",
    "nyc": "latest",
    "prettier-standard": "latest",
    "simple-git-hooks": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "constants.js",
    "generate.js",
    "index.js",
    "inventory.js"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "lint": "standard-markdown README.md && standard",
    "postrelease": "npm run release:tags && npm run release:github && (ci-publish || npm publish --access=public)",
    "prerelease": "npm run update:check && npm run contributors",
    "pretest": "npm run lint",
    "release": "standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "nyc ava",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "license": "MIT",
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/util.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "package.json": [
      "finepack"
    ],
    "*.js,!*.min.js,": [
      "prettier-standard"
    ],
    "*.md": [
      "standard-markdown"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit",
    "pre-commit": "npx lint-staged"
  }
}
